  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "axios-hooks": "^2.2.0",
    "notistack": "^1.0.3",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
