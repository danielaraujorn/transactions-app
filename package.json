    "react-scripts": "3.4.3",
    "react-text-mask": "^5.4.3",
    "text-mask-addons": "^3.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint src --max-warnings=0",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
